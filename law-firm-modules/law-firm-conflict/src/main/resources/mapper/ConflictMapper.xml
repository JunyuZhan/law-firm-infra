<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lawfirm.conflict.mapper.ConflictMapper">
    
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lawfirm.conflict.entity.Conflict">
        <id column="id" property="id"/>
        <result column="conflict_no" property="conflictNo"/>
        <result column="conflict_type" property="conflictType"/>
        <result column="description" property="description"/>
        <result column="party_a" property="partyA"/>
        <result column="party_b" property="partyB"/>
        <result column="discovery_time" property="discoveryTime"/>
        <result column="status" property="status"/>
        <result column="handler_id" property="handlerId"/>
        <result column="handle_opinion" property="handleOpinion"/>
        <result column="handle_time" property="handleTime"/>
        <result column="risk_level" property="riskLevel"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="create_by" property="createBy"/>
        <result column="update_by" property="updateBy"/>
        <result column="deleted" property="deleted"/>
        <result column="remark" property="remark"/>
    </resultMap>
    
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, conflict_no, conflict_type, description, party_a, party_b, discovery_time, status,
        handler_id, handle_opinion, handle_time, risk_level, create_time, update_time,
        create_by, update_by, deleted, remark
    </sql>
    
</mapper>
