{
    "java.compile.nullAnalysis.mode": "disabled",
    "java.jdt.ls.vmargs": "-XX:+UseG1GC -XX:+UseStringDeduplication -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx2G -Xms512m -Xlog:disable",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/target": true,
        "**/bin": true,
        "**/.settings": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.factorypath": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/target": true,
        "**/bin": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/target/**": true,
        "**/bin/**": true
    },
    "java.autobuild.enabled": false,
    "java.configuration.updateBuildConfiguration": "disabled",
    "java.completion.maxResults": 50,
    "editor.suggest.maxVisibleSuggestions": 10,
    "java.debug.settings.maxStringLength": 1000,
    "java.completion.filteredTypes": [
        "java.awt.*",
        "sun.*",
        "jdk.*",
        "org.graalvm.*",
        "io.micrometer.shaded.*"
    ]
}